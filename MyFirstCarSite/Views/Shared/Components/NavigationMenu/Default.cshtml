@model IEnumerable<string>

<a class="btn btn-block btn-secondary" 
   asp-action="List" 
   asp-controller="Vehicle" 
   asp-route-category="">
    Home
</a>

@foreach (string category in Model)
{
    <a class="btn btn-block btn-secondary"
       asp-action="List"
       asp-controller="Vehicle"
       asp-route-category="@category"
       asp-route-vehiclePage="1">

    </a>
}
